<link rel ="stylesheet" type= "text/css" href = "css/new_activity.css">
<h1>List of Events</h1>
<ul><% @events.each do |event| %>
  <li>
   <tr>
     <td><%= event.title %></td>
     <td><%= event.text %></td>
     <td><%= link_to 'Edit', edit_event_path(event) %></td>
   </tr>
  <% end %></li>
</ul>

<div><%= link_to 'Create Event', new_event_path %></div>

